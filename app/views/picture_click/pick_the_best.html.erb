<div class="row bg-info">
  <div class="col-md-4 col-md-push-5">
    <%= link_to "Get Username/Password", users_path(username: "Default"), class: "btn btn-large btn-warning"%>
  </div>
  <div class="col-md-2 col-md-pull-4 col-xs-12">
    <div class="row">
    <% params[:num_of_pics].to_i.times do %>
      <div class="col-xs-2 col-md-12">
        <%= image_tag @pictures.sample, class: "syracuse img-responsive img-rounded img-thumbnail" %>
      </div>
        <% end %>
      </div>
  </div>
</div>
